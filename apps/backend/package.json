{
  "name": "hajk-backend",
  "version": "4.0.0",
  "description": "Hajk Backend",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/hajkmap/Hajk.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.7"
  },
  "type": "module",
  "exports": null,
  "scripts": {
    "start": "node --env-file=.env --experimental-transform-types dist/index.ts",
    "start:prod": "node --env-file=.env --experimental-transform-types index.ts",
    "build": "shx rm -rf dist && shx cp -r server dist",
    "dev": "node --env-file=.env --watch-path=./server --experimental-transform-types server/index.ts",
    "dev:unwatch": "node --env-file=.env --experimental-transform-types server/index.ts",
    "dev:debug": "node --env-file=.env --inspect --watch-path=./server --experimental-transform-types server/index.ts",
    "reset:db": "prisma db push --force-reset && prisma db seed",
    "lint": "eslint ./server"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.21.1",
    "activedirectory2": "^2.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.0.0",
    "express-openapi-validator": "^5.3.7",
    "fast-xml-parser": "^4.5.0",
    "helmet": "^8.0.0",
    "http-proxy-middleware": "^3.0.3",
    "log4js": "^6.9.1",
    "query-string": "^9.1.1",
    "write-excel-file": "^2.0.10",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zod-prisma-types": "^3.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/activedirectory2": "^1.2.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.7.9",
    "@types/ws": "^8.5.12",
    "eslint": "^9.13.0",
    "eslint-plugin-check-file": "^2.8.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "prisma": "^5.21.1",
    "shx": "^0.3.4",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "author": "Jacob Wodzy≈Ñski <jacob.wodzynski@halmstad.se> (https://github.com/hajkmap)"
}

{
  "name": "hajk-backend",
  "version": "4.0.0",
  "description": "Hajk Backend",
  "homepage": ".",
  "repository": {
    "type": "git",
    "url": "https://github.com/hajkmap/Hajk.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.7"
  },
  "type": "module",
  "exports": null,
  "scripts": {
    "start": "node --env-file=.env --experimental-transform-types dist/index.js",
    "build": "shx rm -rf dist && shx cp -r server dist",
    "dev": "node --env-file=.env --watch-path=./server --experimental-transform-types server/index.js",
    "dev:unwatch": "node --env-file=.env --experimental-transform-types server/index.js",
    "dev:debug": "node --env-file=.env --inspect --watch-path=./server --experimental-transform-types server/index.js",
    "reset:db": "prisma db push --force-reset && prisma db seed"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "activedirectory2": "^2.2.0",
    "clf-date": "^0.2.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-openapi-validator": "^5.3.5",
    "fast-xml-parser": "^4.5.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "log4js": "^6.9.1",
    "query-string": "^8.2.0",
    "write-excel-file": "^1.4.29",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "prisma": "^5.19.1",
    "shx": "^0.3.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0"
  },
  "author": "Jacob Wodzy≈Ñski <jacob.wodzynski@halmstad.se> (https://github.com/hajkmap)"
}